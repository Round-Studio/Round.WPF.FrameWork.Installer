<Page x:Class="Round.WPF.FrameWork.Installer.Models.Pages.InstallArticlePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Round.WPF.FrameWork.Installer.Models.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="InstallArticlePage">

    <Grid>
        <Button x:Name="Next" Content="下一步" Margin="0,0,10,10" Height="29" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="65" IsEnabled="False" Click="Next_Click"/>
        <Label Content="用户条款" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontSize="22"/>
        <Button Content="上一步" Margin="0,0,82,10" Height="29" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="65" Click="Button_Click"/>
        <CheckBox x:Name="EnabledNext" Margin="10,0,0,10" HorizontalAlignment="Left" Width="140" Height="30" VerticalAlignment="Bottom" Content="我同意该条款" Click="CheckBox_Checked"/>
        <TextBox x:Name="ArtickeBox" Margin="10,53,10,45" IsReadOnly="True">
        </TextBox>

    </Grid>
</Page>
